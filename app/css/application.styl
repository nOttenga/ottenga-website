
@import '_base'


.square
  background-color rgba(0,0,0,.17)

#main-wrapper
  width 100%
  height 100%
  background-color #FCFCFC

  //height 2500px

.sub-wrapper
  width 100%



/*      HOME       */

#home
  height 100vh
  #home-inner
    height 100%

#parallax
  width 100%
  //height 100vh
  #parallax-image
    position absolute
    top 0
    left 0
    bottom 0
    right 0
    overflow hidden
    img
      height 100%
      max-width initial
      position absolute
      right -150px

  #parallax-mask
    position absolute
    top 100px
    left 0
    bottom 0
    right 0
    border-style solid
    border-width calc(100vh - 100px) 0 0 100vw
    border-color transparent transparent transparent #FCFCFC

.social
  position absolute
  ul
    text-align center
    padding 15px
    li
      width 20px
      padding-bottom 10px


.info
  lost-column 8/12 20px
  lost-move 1/12
  position relative
  z-index 10
  text-align right
  color black
  top 200px
  h1
    font-size 46px
    font-weight 700

  p
    font-size 16px


.home-square
  lost-column 8/12 20px
  lost-move 1/12
  position absolute
  bottom 30px
  #home-square-inner
    width 135px
    height 11px
    background-color black
    position absolute
    right 0


#intro
  width 100%
  overflow hidden
  background-color #FCFCFC
  position relative
  #intro-inner
    lost-column 8/10 20px
    lost-move 1/12
    margin-top 100px
    text-align right
    h2
      color #585858
      font-size 28px
      font-family 'Playfair Display'

    p
      color black
      text-transform uppercase
      font-size 20px
      margin-top 30px
      font-weight 700
    #intro-square
      width 200px
      height 68px
      position absolute
      top 31px
      right -120px


#content
  width 100%
  padding-top 170px
  background-color #FCFCFC
  position relative
  top -2px


#content-title
  lost-column 8/10 20px
  lost-move 1/12
  text-align right
  margin-bottom 100px
  float none !important   /*    RISOLVERE */
  lost-utility: clearfix;

  h3
    color #585858
    font-size 28px
    font-family 'Playfair Display'
    font-weight 700
    span
      font-weight 400


/*************** SQUARES **********/




.project-wrapper
  width: w = 100%
  overflow hidden
  position relative
  .project-inner
    height 100%
    width w
  .project-inner-left
    background-position center
    background-size cover
  .project-inner-right
    position absolute
    top 0
    left w
    z-index 10
    transition all .3s ease-in-out
    background-color #FFFFFF
    text-align center
    .project-inner-square
      width 45px
      height 11px
      background-color black
      position relative
      margin 44px auto 45px auto
    .project-inner-list
      lost-column 6/10 0
      lost-move 2/10
      position absolute
      //margin 0 auto

    ul
      display inline-block


      li
        font-family 'Playfair Display'
        font-size 28px
        color #585858
        text-align left
        padding-bottom 10px
    .project-inner-link
      lost-column 1/1
      position absolute
      bottom 45px
      font-family 'azedobold'
      font-size 28px
      a
        color #585858
        &:visited
          opacity .5
  &:hover
    .project-inner-right
      left 0



#experientia
  height 350px
  //max-width 433px
  .project-inner-left
    background-image url('../images/exp-main.jpg')
