#footer
  width 100%
  #footer-utility
    height 400px
    width 100%
    font-family 'Playfair Display'
    text-align center
    color #585858
    #footer-social
      margin-top 150px
      lost-align center
      ul
        height 50px
        li
          display inline-block
          padding 0 10px
          img
            width 20px
            height 20px

    #footer-top
      padding-top 100px
      cursor pointer
      transition all 1s ease
      &:hover
        text-decoration line-through
    #footer-imprint
      padding-top 80px
      a
        text-decoration none
        color #585858
        &:hover
          text-decoration line-through
  #footer-image
    width 100%
    text-align right






