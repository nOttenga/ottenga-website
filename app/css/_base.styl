
@import url('https://fonts.googleapis.com/css?family=Raleway:400,300,700');
@import url('https://fonts.googleapis.com/css?family=Playfair+Display:400,700');

@font-face {
    font-family: 'azedobold';
    src: url('/font/azedo-bold-webfont.eot');
    src: url('/font/azedo-bold-webfont.eot?#iefix') format('embedded-opentype'),
         url('/font/azedo-bold-webfont.woff2') format('woff2'),
         url('/font/azedo-bold-webfont.woff') format('woff'),
         url('/font/azedo-bold-webfont.ttf') format('truetype'),
         url('/font/azedo-bold-webfont.svg#azedobold') format('svg');
    font-weight: bold;
    font-style: bold;

}




@font-face {
    font-family: 'azedolight';
    src: url('/font/azedo-light-webfont.eot');
    src: url('/font/azedo-light-webfont.eot?#iefix') format('embedded-opentype'),
         url('/font/azedo-light-webfont.woff2') format('woff2'),
         url('/font/azedo-light-webfont.woff') format('woff'),
         url('/font/azedo-light-webfont.ttf') format('truetype'),
         url('/font/azedo-light-webfont.svg#azedolight') format('svg');
    font-weight: normal;
    font-style: normal;

}



$secondaryCol = #D04036


html, body
	font-family 'Raleway'
	font-weight 400
	//lost-utility edit
	border 0
	margin 0
	width 100%
	height 100%
	font-size 16px
	font-weight normal
	color white
	font-smooth: always;
	box-sizing: border-box;

img
	max-width 100%

ul
	list-style none
	padding 0
	margin 0

h1,h2,h3,h4,p
	font-size 18px
	font-weight 400
	padding 0
	margin 0


body
	background-color #FCFCFC
